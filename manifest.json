{
    "name": "test",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "in-content.js"
            ]
        }
    ],
    "browser_action": {
        "default_title": "test",
        "default_popup": "popup.html"
    },
        "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'; img-src * data: 'self' 'unsafe-eval'",
    "permissions": [
        "tabs",
        "storage"
    ]
}